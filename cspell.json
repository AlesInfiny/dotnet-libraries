{
    "languageSettings": [
        {
            // VSCode languageId. i.e. typescript, java, go, cpp, javascript, markdown, latex
            // * will match against any file type.
            "languageId": "*",
            // Language locale. i.e. en-US, de-AT, or ru. * will match all locales.
            // Multiple locales can be specified like: "en, en-US" to match both English and English US.
            "locale": "*",
            // To exclude patterns, add them to "ignoreRegExpList"
            "ignoreRegExpList": [],
            // List of dictionaries to enable by name in `dictionaryDefinitions`
            "dictionaries": ["csharp"],
            // Dictionary definitions can also be supplied here. They are only used if "languageId" and "locale" match.
            "dictionaryDefinitions": []
        }
    ],
    "ignorePaths": [
        ".vscode",
        ".git",
        ".gitattributes",
        ".gitignore",
        ".github/workflows/**",
        ".markdownlint-cli2.yaml",
        ".markdownlint.yaml",
        ".textlintignore",
        ".textlintrc",
        ".editorconfig",
        "cspell.json",
        "Directory.Build.props",
        "Directory.Packages.props",
        "LICENSE",
        "dotnet-libraries.code-workspace",
        "package-lock.json",
        "package.json",
        "stylecop.json",
        "**/node_modules/**",
        "**/*.csproj",
        "**/*.Designer.cs",
        "**/*.resx"
    ],
    "words": [
        "AlesInfiny",
        "BIPROGY",
        "textlint",
        "winbatch",
        "Xunit"
    ]
}